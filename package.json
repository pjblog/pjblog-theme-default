{
  "name": "pjblog-theme-default",
  "version": "2.3.0",
  "description": "PJBlog 默认主题",
  "main": "./build/index.js",
  "type": "module",
  "files": [
    "build",
    "dist"
  ],
  "scripts": {
    "dev": "node --loader ts-node/esm src/serve.ts",
    "preview": "node build/preview.js",
    "build": "npm run build:client && npm run build:server",
    "build:client": "rm -rf ./dist && vite build && node scripts/css.js",
    "build:server": "rm -rf ./build && vite build --ssr"
  },
  "dependencies": {
    "@ant-design/icons": "^5.3.6",
    "@fingerprintjs/fingerprintjs": "^4.2.2",
    "@pjblog/blog": "^2.3.32",
    "@pjblog/vite-middleware": "^2.3.1",
    "@zille/application": "^1.1.2",
    "@zille/configurator": "^1.1.2",
    "@zille/core": "^1.1.2",
    "@zille/http": "^1.1.12",
    "@zille/service": "^1.1.2",
    "antd": "^5.16.0",
    "axios": "^1.6.8",
    "classnames": "^2.5.1",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.10",
    "koa": "^2.15.2",
    "koa-connect": "^2.1.0",
    "koa-send": "^5.0.1",
    "marked": "^12.0.1",
    "md-editor-rt": "^4.12.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite": "^4.5.3"
  },
  "devDependencies": {
    "@ant-design/static-style-extract": "^1.0.2",
    "@types/classnames": "^2.3.1",
    "@types/crypto-js": "^4.2.2",
    "@types/koa": "^2.15.0",
    "@types/react": "^18.2.73",
    "@types/react-dom": "^18.2.23",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "less": "^4.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.3"
  }
}